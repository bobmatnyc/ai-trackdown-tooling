{
  "name": "ai-trackdown-tooling",
  "version": "1.0.0",
  "description": "Professional CLI tool for ai-trackdown functionality",
  "type": "module",
  "bin": {
    "aitrackdown": "./dist/index.js",
    "atd": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "biome check .",
    "format": "biome format . --write",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "version:sync": "tsx scripts/version-sync.ts",
    "version:patch": "tsx scripts/release.ts patch",
    "version:minor": "tsx scripts/release.ts minor",
    "version:major": "tsx scripts/release.ts major",
    "release:patch": "tsx scripts/release.ts patch --push",
    "release:minor": "tsx scripts/release.ts minor --push",
    "release:major": "tsx scripts/release.ts major --push"
  },
  "keywords": [
    "cli",
    "aitrackdown",
    "ai",
    "project-management",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "conventional-changelog": "^7.1.0",
    "conventional-changelog-cli": "^5.0.0",
    "figlet": "^1.8.0",
    "gray-matter": "^4.0.3",
    "inquirer": "^12.1.0",
    "js-yaml": "^4.1.0",
    "ora": "^8.1.1",
    "semver": "^7.7.2",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@types/figlet": "^1.7.0",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^24.0.10",
    "@types/semver": "^7.7.0",
    "@vitest/ui": "^3.2.4",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
